{
    "version": 2,
    "framework": null,
    "functions": {
      "public/index.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://tuo-dominio.vercel.app"
    }
  }